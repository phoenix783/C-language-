#include<stdio.h>
int lcm(int,int);
{ int x;
  int y;
  
  printf("enter the value of x");
  scanf("%d",&x);
  printf("enter the value of y");
  scanf("%d",&y);
  printf("lcm=%d",lcm(x,y));
}
 int lcm(int a,int b)
 { 
   static int m=0;
   m=m+b;
   if(m%a==0&&m%b==0)
     return m;
      return lcm(a,b)

   
